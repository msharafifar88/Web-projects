<div class="row">
    <div class="col-md-12">
        <h1>{{title}}<a href="/images/add" class="btn btn-success pull-right" style="margin-top:5px;">Add&nbsp;New&nbsp;Images</a></h1>
        <hr />
    </div>

    <div class="col-md-4">
        {{#each images}}
            <div class="col-md-4">
                <img src="/images/uploaded/{{this}}" class="img-responsive img-thumbtail" />
            </div>
        {{else}}
            <div class="col-md-12 text-center">
                <strong>No Images Available</strong>
            </div>
        {{/each}}
    </div>
</div>